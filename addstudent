    private void jTextField3ActionPerformed(java.awt.event.ActionEvent evt) {                                            
          if( isValid(jTextField3.getText())==false)
           jLabel7.setText("wrong format try again");
      else if( isValid(jTextField3.getText())==true)
           jLabel7.setText("good");
    }                                           

