    private void jPasswordField2ActionPerformed(java.awt.event.ActionEvent evt) {                                                
        String s =Arrays.toString(jPasswordField1.getPassword());
          String a =Arrays.toString(jPasswordField2.getPassword());
       
     if( s == null ? a != null : !s.equals(a))
             jLabel16.setText("passowrd not match");
     else if (s == null ? a == null : s.equals(a))
          jLabel16.setText("passowrd match");
    }                                               

