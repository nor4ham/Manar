    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
      /*  if( isValid(jTextField3.getText())==false){
           jLabel7.setText("wrong format try again");}
       String s =jPasswordField1.getText();
       
       if(s.compareToIgnoreCase(jPasswordField2.getText())==1){
             jLabel16.setText("passowrd not match");}
       if (isValid(jTextField3.getText())==true &&s.compareToIgnoreCase(jPasswordField2.getText())!=1){
*/
      try {
query="insert into STUDENTS values(?,?,?,?,?,?,?,?,?);";
   preparedstatement= connection.prepareStatement(query);   
 preparedstatement.setString(1, jTextField5.getText());
  preparedstatement.setString(2, jComboBox2.getSelectedItem().toString());
    preparedstatement.setString(3, jTextField1.getText());
      preparedstatement.setString(4, jTextField2.getText());
 preparedstatement.setString(5, jTextField3.getText());
  preparedstatement.setString(6, jTextField4.getText());
    preparedstatement.setString(7, jComboBox1.getSelectedItem().toString());
 int LEVEL =Integer.parseInt(jTextField6.getText());
  preparedstatement.setInt(8, LEVEL);
  String password =new String(jPasswordField1.getPassword());  
  int PASSSWORD =Integer.parseInt(password);;
  preparedstatement.setInt(9, PASSSWORD);
preparedstatement.execute();
 preparedstatement.close();
            JOptionPane.showMessageDialog(this, "Record saved.");
            setVisible(false);
             new LogIn().setVisible(true);   
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());jTextField5.setText("");
        }
      
       
       
    }                                        

