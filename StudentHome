    public void doConnect() {
        Security SecurityConnection = new Security();
        try {
            connection = SecurityConnection.connect();
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());

        }
        try {
            query = " select * from now";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            user = resultSet.getString("user");
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());

        }
        try {
            query = " select * from students where USERNAMe='" + user + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            FIRSTNAME = resultSet.getString("FIRSTNAME");
            LASTNAME = resultSet.getString("LASTNAME");
            level = resultSet.getInt("level");
            INTRESTEDCOURSE = resultSet.getString("INTRESTEDCOURSE");
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());

        }
        try {
            query = " select * from cs where USERNAMe='" + user + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            C1 = resultSet.getString("COURSEID");
            resultSet.next();
            C2 = resultSet.getString("COURSEID");
            resultSet.next();
            C3 = resultSet.getString("COURSEID");
            resultSet.next();
            C4 = resultSet.getString("COURSEID");
            resultSet.next();
            C5 = resultSet.getString("COURSEID");
            resultSet.next();
            C6 = resultSet.getString("COURSEID");
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());

        }
        try {

            query = " select * from tasks where COURSE='" + C1 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            query = " select * from tasks where COURSE='" + C2 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            query = " select * from tasks where COURSE='" + C3 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);

            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            query = " select * from tasks where COURSE='" + C4 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            query = " select * from tasks where COURSE='" + C5 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            query = " select * from tasks where COURSE='" + C6 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode2.addElement(resultSet.getString("COURSE") + " " + resultSet.getString("decription")
                        + " " + resultSet.getString("link"));
            }

            AssignmentsList.setModel(mode2);
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());
        }
        try {
            TestjList.setModel(model);
            query = " select * from COURSES where COURSEID='" + C1 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("1 : " + resultSet.getString("TESTS"));
            c1w = resultSet.getString("time");
            c1info = resultSet.getString("information");
            query = " select * from COURSES where COURSEID='" + C2 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("2 : " + resultSet.getString("TESTS"));
            c2w = resultSet.getString("time");
            c2info = resultSet.getString("information");
            query = " select * from COURSES where COURSEID='" + C3 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("3 : " + resultSet.getString("TESTS"));
            c3w = resultSet.getString("time");
            c3info = resultSet.getString("information");

            query = " select * from COURSES where COURSEID='" + C4 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("4: " + resultSet.getString("TESTS"));
            c4info = resultSet.getString("information");
            c4w = resultSet.getString("time");
            query = " select * from COURSES where COURSEID='" + C5 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("5 : " + resultSet.getString("TESTS"));
            c5info = resultSet.getString("information");
            c5w = resultSet.getString("time");
            query = " select * from COURSES where COURSEID='" + C6 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            resultSet.next();
            model.addElement("6 : " + resultSet.getString("TESTS"));
            c6info = resultSet.getString("information");
            c6w = resultSet.getString("time");
            TestjList.setModel(model);
        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());
        }

         //materials
        try {

            query = " select * from materials where COURSE='" + C1 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C1 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }

            query = " select * from materials where COURSE='" + C2 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C2 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }

            query = " select * from materials where COURSE='" + C3 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C3 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }

            query = " select * from materials where COURSE='" + C4 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C4 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }

            query = " select * from materials where COURSE='" + C5 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C4 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }

            query = " select * from materials where COURSE='" + C6 + "'";
            preparedstatement = connection.prepareStatement(query);
            resultSet = preparedstatement.executeQuery(query);
            while (resultSet.next()) {
                mode3.addElement(C6 + "article: " + resultSet.getString("article") + "video: " + resultSet.getString("video"));
            }
             jListmaterial.setModel(mode3);


        } catch (SQLException sqlEx) {
            JOptionPane.showMessageDialog(this, sqlEx.getMessage());
        }

    }

