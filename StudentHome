    int level;
    String C1 = "", C2 = "", C3 = "", C4 = "", C5 = "", C6 = "";
    String c1info = "", c2info = "", c3info = "", c4info = "", c5info = "", c6info = "";
    String c1i = "", c2i = "", c3i = "", c4i = "", c5i = "", c6i = "";
    String c1v = "", c2v = "", c3v = "", c4v = "", c5v = "", c6v = "";
    String c1a = "", c2a = "", c3a = "", c4a = "", c5a = "", c6a = "";
    String c1w = "", c2w = "", c3w = "", c4w = "", c5w = "", c6w = "";
    ResultSet resultSet;
    PreparedStatement preparedstatement;
    DefaultListModel model = new DefaultListModel();
    DefaultListModel mode2 = new DefaultListModel();
