    private void MyProfileLablePanelMouseClicked(java.awt.event.MouseEvent evt) {                                                 

        MyProfilePanel.setSize(570, 575);
        MyProfilePanel.setVisible(true);
        //My Platform
        AssignmentsPanel.setVisible(false);
        Course1.setVisible(false);
        Course6.setVisible(false);
        Course3.setVisible(false);
        Course5.setVisible(false);
        Course2.setVisible(false);
        TestPanel.setVisible(false);
        Course4.setVisible(false);


    }                                                

    private void HomeLabelPanelMouseClicked(java.awt.event.MouseEvent evt) {                                            
        //home
        CouresName1.setText("CouresName1 :" + C1 + " ");
        c2.setText("CouresName2:" + C2 + "");
        c3.setText("CouresName3 :" + C3 + "");
        c4.setText("CouresName4 :" + C4 + "");
        c5.setText("CouresName5 :" + C5 + "");
        c6.setText("CouresName6 :" + C6 + "");
        by1.setText(c1info);
        by2.setText(c2info);
        by3.setText(c3info);
        by4.setText(c4info);
        by5.setText(c5info);
        by6.setText(c6info);
        a1.setText(c1w);
        a2.setText(c2w);
        a3.setText(c3w);
        a4.setText(c4w);
        a5.setText(c5w);
        a6.setText(c6w);
        MyProfilePanel.setVisible(false);
        AssignmentsPanel.setVisible(false);
        MySchedulePanel.setVisible(false);
        Course1.setVisible(true);
        Course6.setVisible(true);
        Course3.setVisible(true);
        Course5.setVisible(true);
        Course2.setVisible(true);
        TestPanel.setVisible(false);
        Course4.setVisible(true);

    }                                           

